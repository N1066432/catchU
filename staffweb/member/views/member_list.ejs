        <!-- //Fonts -->
    </head>
    
    <body>
        <!-- mian-content -->
        <div class="main-content" id="home">
            <!-- header -->
            <header class="header">
                <div class="container">
                    <!-- nav -->
                    <nav class="py-2">
                        <div id="logo">
                            <h1> <a class="navbar-brand" href="/user/login/form">桌助你<span>catchU</span> </a>
                            </h1>
                        </div>
    
                        <label for="drop" class="toggle">Menu</label>
                        <input type="checkbox" id="drop" />
                        <ul class="menu">
                            <li class="active"><a href="/user/login/form">返回首頁</a></li>
                           
                            <li class="dropdown">
                                <!-- First Tier Drop Down -->
                                <label for="drop-2" class="toggle"> <span class="fa fa-angle-down" aria-hidden="true"></span> </label>
                                
                                <input type="checkbox" id="drop-2" />
                               
                            </li>
                           
    
                            <li class="header-right">
                                <ul class="list-unstyled team-icons mt-4">
                                    <li>
                                        <a href="#" class="t-icon">
                                            <span class="fa fa-facebook-f"></span>
                                        </a>
                                    </li>
                                    <li>
                                        <a href="#" class="t-icon">
                                            <span class="fa fa-twitter"></span>
                                        </a>
                                    </li>
    
    
                                </ul>
                            </li>
    
                        </ul>
                    </nav>
                    <!-- //nav -->
                </div>
            </header>
        
        <!--~~~~~~~~~~~~~~~~~--> 
        <section class="contact" id="contact">
            <div class="banner-w3pvt-info ">
                <div class="overlay-w3ls">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="banner-content text-center">
                                    <h3>會員資料</h3>
                                   
                                </div>
             <a href="/member/remove/form" class="btn">刪除會員</a>
             <a href="/member/update/form" class="btn">修改會員</a>

             <table class="table">
                    <thead>
                        <tr class="table-light" style="text-align:center;">
                            <th width="15%">會員帳號</th>
                            <th width="15%">會員名稱</th>
                            <th width="20%">會員點數</th>
                            <th width="10%">性別</th>
                            <th width="20%">生日</th>
                            <th width="20%">建立日期</th>                        
                        </tr>
                    </thead>
                    <tbody>
                        
                            <tr class="table-light">
                                <td align="center" style="vertical-align:middle;"><%= items.memberphone %></a></td>
                                <td align="center" style="vertical-align:middle;"><%= items.membername %></td>
                                <td align="center" style="vertical-align:middle;"><%= items.points %></td>
                                <td align="center" style="vertical-align:middle;"><%= items.gender %></td> 
                                <td align="center" style="vertical-align:middle;"><%= items.birthday %></td>
                                <td align="center" style="vertical-align:middle;"><%= items.creationdate %></td>                     
                            </tr> 

                    </tbody>
                </table>   
              
               
        </div>       
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!--~~~~~~~~~~~~~~~~~--> 
